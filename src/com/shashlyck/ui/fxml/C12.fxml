<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="parent" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.shashlyck.ui.fxml.C12">
    <Label alignment="CENTER" contentDisplay="CENTER" text="Шаг 2:">
        <font>
          <Font size="36.0" />
        </font>
    </Label>
    <Label alignment="CENTER" contentDisplay="CENTER" text="Введите все точки (x;y)" textAlignment="CENTER" wrapText="true">
        <font>
          <Font size="22.0" />
        </font>
    </Label>
    <HBox alignment="CENTER" VBox.vgrow="NEVER">
        <Label text="Кол-во точек:">
           <font>
               <Font name="System Italic" size="21.0" />
           </font>
           <HBox.margin>
               <Insets right="24.0" />
           </HBox.margin>
        </Label>
        <Label fx:id="amountLabel" text="Amount">
           <font>
               <Font name="System Italic" size="21.0" />
           </font>
        </Label>
    </HBox>
    <TableView fx:id="pointsTableView" editable="true" VBox.vgrow="ALWAYS">
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy></TableView>
   <padding>
      <Insets top="28.0" />
   </padding>
</VBox>
